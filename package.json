{
  "name": "cvs-svc-metrics-collector",
  "version": "1.0.0",
  "description": "Collects metrics from dynamodb",
  "author": "Martin Kemp <martinkemp@deloitte.co.uk>",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/dvsa/cvs-svc-metrics-collector.git"
  },
  "scripts": {
    "audit-dep": "yarn audit",
    "build": "npx tsc --rootDir ./ --outDir .build/ --sourceMap false",
    "clean": "rm -rf .build/",
    "coverage": "jest --coverage",
    "local-package": "yarn run build && yarn run package",
    "package": "mkdir ${ZIP_NAME} && cp package.json yarn.lock ${ZIP_NAME} && cp -r .build/src/* ${ZIP_NAME}/ && cd ${ZIP_NAME} && yarn install --prod && rm package.json yarn.lock && zip -qr ../${ZIP_NAME}.zip . && cd .. && rm -rf ${ZIP_NAME}",
    "sonar-scanner": "echo 'not implemented'",
    "test": "jest -c jest.config.js",
    "test-i": "jest -c jest.config.int.js"
  },
  "main": "src/handler.js",
  "devDependencies": {
    "@types/aws-lambda": "^8.10.31",
    "@types/jest": "^24.0.18",
    "@types/lambda-tester": "^3.6.0",
    "@types/luxon": "^1.15.2",
    "@types/node": "^12.7.5",
    "jest": "^24.9.0",
    "jest-sonar-reporter": "^2.0.0",
    "lambda-tester": "^3.6.0",
    "ts-jest": "^24.1.0",
    "tslint": "^5.20.0",
    "tslint-jsdoc-rules": "^0.2.0",
    "typescript": "^3.6.3"
  },
  "dependencies": {
    "aws-lambda": "^0.1.2",
    "aws-sdk": "^2.530.0",
    "aws-xray-sdk": "^2.3.6",
    "luxon": "^1.17.3",
    "typescript-logging": "^0.6.3"
  }
}
